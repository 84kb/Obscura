{
  "name": "obscura",
  "version": "0.6.5",
  "description": "ライブラリ型メディアプレイヤー",
  "main": "dist-electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "bump": "node scripts/bump-version.js",
    "build": "npm run bump && tsc && vite build && electron-builder",
    "preview": "vite preview",
    "electron:dev": "vite",
    "electron:build": "npm run bump && vite build && electron-builder"
  },
  "keywords": [
    "media",
    "player",
    "library",
    "electron"
  ],
  "author": "84kb",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/84kb/Obscura.git"
  },
  "devDependencies": {
    "@types/chokidar": "^1.7.5",
    "@types/cors": "^2.8.19",
    "@types/discord-rpc": "^4.0.10",
    "@types/express": "^5.0.6",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "electron": "^28.1.0",
    "electron-builder": "^26.4.0",
    "ffmpeg-static": "^5.3.0",
    "ffprobe-static": "^3.1.0",
    "local-ssl-proxy": "^2.0.5",
    "selfsigned": "^5.5.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-electron": "^0.28.1",
    "vite-plugin-electron-renderer": "^0.14.5",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@types/multer": "^2.0.0",
    "chokidar": "^3.6.0",
    "cors": "^2.8.5",
    "discord-rpc": "^4.0.1",
    "electron-log": "^5.4.3",
    "electron-updater": "^6.7.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "fs-extra": "^11.3.3",
    "helmet": "^8.1.0",
    "multer": "^2.0.2",
    "nanoid": "^3.3.11",
    "node-machine-id": "^1.1.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-virtuoso": "^4.18.1",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
    "virtua": "^0.48.3"
  },
  "build": {
    "appId": "com.obscura.medialib",
    "productName": "Obscura",
    "directories": {
      "output": "release"
    },
    "artifactName": "${productName}-Setup-${version}.${ext}",
    "files": [
      "dist/**/*",
      "dist-electron/**/*"
    ],
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "build/icon.ico"
    },
    "nsis": {
      "oneClick": true,
      "perMachine": false,
      "deleteAppDataOnUninstall": false
    },
    "extraResources": [
      {
        "from": "node_modules/ffmpeg-static/ffmpeg.exe",
        "to": "bin/ffmpeg.exe"
      },
      {
        "from": "node_modules/ffprobe-static/bin/win32/x64/ffprobe.exe",
        "to": "bin/ffprobe.exe"
      }
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "84kb",
        "repo": "Obscura"
      }
    ]
  }
}
